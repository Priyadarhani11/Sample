<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="student.headless.builder.model.student" />
	<class name="student.headless.builder.model.impl.studentImpl" table="STU_student">
		<id access="com.liferay.portal.dao.orm.hibernate.MethodPropertyAccessor" column="id_" name="id" type="long">
			<generator class="increment" />
		</id>
		<property access="com.liferay.portal.dao.orm.hibernate.MethodPropertyAccessor" name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.MethodPropertyAccessor" name="standard" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>